{
    "name": "retail-pos-monorepo",
    "version": "0.0.1",
    "private": true,
    "workspaces": [
        "apps/*"
    ],
    "scripts": {
        "dev": "docker compose up",
        "start": "pm2 start ecosystem.config.js",
        "stop": "pm2 delete ecosystem.config.js",
