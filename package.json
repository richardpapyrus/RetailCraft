        "test": "turbo run test",
        "lint": "turbo run lint"
    },
    "devDependencies": {
        "eslint": "latest",
        "pm2": "^6.0.14",
        "prettier": "latest",
        "turbo": "latest"
    },
    "packageManager": "npm@11.6.2"
}